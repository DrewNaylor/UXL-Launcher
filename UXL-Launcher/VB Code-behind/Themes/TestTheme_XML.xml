<UXL_Launcher_Theme>
  <Title>Eyesore 2</Title>
  <Description>Originally a test theme for UXL Launcher, Eyesore 2 is meant to be a spiritual successor to the Windows 3.1 Hotdog Stand theme, but worse.</Description>
  <Author>Drew Naylor</Author>
  
    <Theme_Colors>
      
      <Button>
        <BackColor>Maroon</BackColor>
        <ForeColor>White</ForeColor>
      </Button>
      
      <GroupBox>
        <BackColor>Green</BackColor>
        <ForeColor>Blue</ForeColor>
      </GroupBox>

      <FlowLayoutPanel>
        <BackColor>Yellow</BackColor>
        <ForeColor>Purple</ForeColor>
      </FlowLayoutPanel>
      
      <MenuBar>
        <BackColor>Purple</BackColor>
      </MenuBar>

      <StatusBar>
        <BackColor>CadetBlue</BackColor>
      </StatusBar>

      <Label>
        <BackColor>Maroon</BackColor>
        <ForeColor>White</ForeColor>
      </Label>

      <TextBox>
        <BackColor>Beige</BackColor>
        <ForeColor>DarkViolet</ForeColor>
      </TextBox>

      <MenuItem>
        <BackColor>Purple</BackColor>
        <ForeColor>AntiqueWhite</ForeColor>
      </MenuItem>

      <StatusLabel>
        <BackColor>Maroon</BackColor>
        <ForeColor>White</ForeColor>
        <BorderSides>All</BorderSides>
        <BorderStyle>SunkenInner</BorderStyle>
      </StatusLabel>
      
    </Theme_Colors>
  
</UXL_Launcher_Theme>